{
    "name": "j6s/shape-up-downloader",
    "description": "Downloads basecamps shape-up in other formats",
    "type": "project",
    "license": ["MIT"],
    "require": {
        "php": "^8.0",
        "symfony/console": "^6.0",
        "symfony/dom-crawler": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/cache": "^6.0",
        "thecodingmachine/safe": "^2.1",
        "ext-dom": "*",
        "symfony/dependency-injection": "^6.0",
        "symfony/config": "^6.0",
        "symfony/finder": "^6.0"
    },
    "authors": [
        {
            "name": "Johannes Hertenstein",
            "email": "hello@j6s.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "J6s\\ShapeUpDownloader\\": "src"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.10"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.10",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "analyze": [ "psalm", "phpcs" ]
    }
}
