{
    "name": "j6s/shape-up-downloader",
    "description": "Downloads basecamps shape-up in other formats",
    "type": "project",
    "license": ["MIT"],
    "require": {
        "php": "^8.4",
        "symfony/console": "^7.3",
        "symfony/dom-crawler": "^7.3",
        "symfony/css-selector": "^7.3",
        "symfony/cache": "^7.3",
        "thecodingmachine/safe": "^3.3",
        "ext-dom": "*",
        "symfony/dependency-injection": "^7.3",
        "symfony/config": "^7.3",
        "symfony/finder": "^7.3"
    },
    "authors": [
        {
            "name": "Johannes Hertenstein",
            "email": "hello@j6s.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "J6s\\ShapeUpDownloader\\": "src"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.13",
        "phpstan/phpstan": "^2.1",
        "thecodingmachine/phpstan-safe-rule": "^1.4",
        "spaze/phpstan-disallowed-calls": "^4.6"
    },
    "scripts": {
        "analyze": [ "psalm", "phpcs" ]
    }
}
